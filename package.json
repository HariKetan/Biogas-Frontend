    "ajv-keywords": "^5.1.0",
    "apexcharts": "^3.54.1",
    "axios": "^1.7.9",
    "change-case": "^5.4.4",
    "date-fns": "^3.6.0",
    "history": "^5.3.0",
    "igniteui-react-gauges": "^18.9.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lodash": "^4.17.21",
    "numeral": "^2.0.6",
    "prop-types": "^15.8.1",
